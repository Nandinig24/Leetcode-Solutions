class Solution {
public:
    vector<int> findErrorNums(vector<int>& nums) {
        int n=nums.size();
        vector<int>hsh(n,0);
        vector<int>v(2,0);
        for(int i=0;i<nums.size();i++){
            hsh[nums[i]-1]++;
        }
        for(int i=0;i<n;i++){
            if(hsh[i]==2)
            v[0]=i+1;
            else if(hsh[i]==0)
            v[1]=i+1;
        }
        return v;
        
    }
};