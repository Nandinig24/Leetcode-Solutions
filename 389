class Solution {
public:
    char findTheDifference(string s, string t) {
        
int hsh[26]={0};
int hsh1[26]={0};
char c;
for(int i=0;i<s.size();i++){
    hsh[s[i]-'a']++;
}
for(int i=0;i<t.size();i++){
    hsh1[t[i]-'a']++;
}
for(int i=0;i<26;i++){
    if(hsh[i]!=hsh1[i]){
  c='a'+i;
//   break;


}
}
return c;

      
    }
};