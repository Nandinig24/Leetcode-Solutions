class Solution {
public:
    string clearDigits(string s) {
        
        stack<char>st;
        string ans="";
        for(int i=0;i<s.size();i++){
            if(s[i]>='a' && s[i]<='z')
                st.push(s[i]);
            else  if(s[i]>='0' && s[i]<='9')
                st.pop();
        }
        
        while(!st.empty()){
            ans=st.top()+ans;
            st.pop();
        }
        return ans;
    }
};