class Solution {
public:
    int findSpecialInteger(vector<int>& arr) {
        unordered_map<int,int>mp;
        for(int i=0;i<arr.size();i++){
            mp[arr[i]]++;
        }
        int mx=-1;
        int number=0;
        for(auto i:mp){
            if(i.second>mx){
                number=i.first;
                mx=i.second;
            }


        }
        return number;

        
    }
};